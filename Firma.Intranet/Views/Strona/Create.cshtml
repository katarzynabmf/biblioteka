@model Firma.Data.Data.CMS.Strona

@{
    ViewData["Title"] = "Create";
}

<h1>Dodaj</h1>

<hr />

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label asp-for="LinkTytul" class="control-label"></label>
                <input asp-for="LinkTytul" class="form-control" />
                <span asp-validation-for="LinkTytul" class="text-danger"></span>
            </div>
        </div>
        <div class="col">
            <div class="form-group">
                <label asp-for="Tytul" class="control-label"></label>
                <input asp-for="Tytul" class="form-control" />
                <span asp-validation-for="Tytul" class="text-danger"></span>
            </div>
        </div>
        <div class="col">
            <div class="form-group">
                <label asp-for="Pozycja" class="control-label"></label>
                <input asp-for="Pozycja" class="form-control" />
                <span asp-validation-for="Pozycja" class="text-danger"></span>
            </div>
            </div>
        </div>
        <div class="row">

            <div class="col">
                <div class="form-group">
                    <label asp-for="Tresc" class="control-label"></label>
                    <input asp-for="Tresc" class="form-control" />
                    <span asp-validation-for="Tresc" class="text-danger"></span>
                </div>
            </div>

        </div>
        <div class="row">
        <div class="col">
            <div class="form-group btn-group shadow-0" role="group" aria-label="Basic example">
                <input type="submit" value="Utwórz" class="btn btn-outline-dark" data-mdb-color="dark" />
                <a asp-action="Index" class="btn btn-outline-dark" data-mdb-color="dark">Wróć</a>
            </div>
        </div>
    </div>
</form>
    



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
